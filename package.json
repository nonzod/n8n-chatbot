{
  "name": "tt-chat-n8n",
  "version": "0.6.2",
  "type": "module",
  "description": "A simplified chat widget for n8n",
  "homepage": "https://www.npmjs.com/package/tt-chat-n8n",
  "author": {
    "name": "Nicola Tomassoni",
    "email": "io@nicolatomassoni.it",
    "url": "https://www.nicolatomassoni.it"
  },
  "contributors": [
    {
      "name": "TourTools Team",
      "email": "coder@tourtools.it",
      "url": "https://www.tourtools.it"
    }
  ],
  "main": "dist/tt-chat.umd.js",
