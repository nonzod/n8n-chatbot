    "vue-markdown-render": "^2.2.1",
    "vue-select": "^4.0.0-beta.6"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.3.0",
    "sass": "^1.87.0",
    "terser": "^5.39.0",
    "typescript": "^5.1.0",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^3.3.0",
    "vue-tsc": "^1.8.0"
  },
  "files": [
    "dist",
    "README.md",
    "resources/screenshot.png"
